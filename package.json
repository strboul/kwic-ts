{
  "name": "@strboul/kwic-ts",
  "version": "0.1.0",
  "description": "Locate keywords-in-contexts",
  "author": "Metin Yazici",
  "main": "dist/kwic.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strboul/kwic-ts.git"
  },
  "bugs": {
    "url": "https://github.com/strboul/kwic-ts/issues"
  },
  "homepage": "https://github.com/strboul/kwic-ts#readme",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:debug-devtools": "node --inspect $(npm bin)/jest --no-cache --runInBand",
    "test:debug-repl": "node inspect $(npm bin)/jest --runInBand",
    "lint": "eslint src/** --ext .ts",
    "prettierCheck": "prettier --list-different '{,!(build|coverage|dist|example)/**/}*.{css,js,jsx,ts}'",
    "coverage": "jest --coverage",
    "clean": "rm -rf dist",
    "npmLink": "npm link '@strboul/kwic-ts'",
    "npmPublish": "npm login && npm publish --access public"
  },
  "devDependencies": {
    "@types/jest": "*",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "babel-jest": "*",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "*",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "*",
    "ts-jest": "*",
    "ts-node": "*",
    "typescript": "*"
  }
}
